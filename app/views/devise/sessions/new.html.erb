<h2>ログイン</h2>

<%= form_with model:@user, url: user_session_path, local: true do |f| %>
   <%= render "devise/shared/error_messages", resource: resource %> 

  <div class="field">
    メールアドレス
    <%= f.email_field :email, autofocus: true %>
  </div>

  <div class="field">
    パスワード
    <%= f.password_field :password %>
  </div>

  <div class="actions">
    <%= f.submit "ログイン" %>
  </div>
<% end %>

